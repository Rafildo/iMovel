<UserControl x:Class="iMovel3.AppPage3"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iMovel3"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="900">
        <Grid HorizontalAlignment="Left" Height="360" Margin="325,150,0,0" VerticalAlignment="Top" Width="250">
            <TextBox x:Name="UsernameTextBox" HorizontalAlignment="Left" Height="23" Margin="80,116,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
            <PasswordBox x:Name="PasswordTextBox" HorizontalAlignment="Left" Margin="80,170,0,0" VerticalAlignment="Top" Width="160"/>
            <Button x:Name="LoginButton" Content="Entrar" HorizontalAlignment="Left" Margin="110,255,0,0" VerticalAlignment="Top" Width="130" Click="LoginButton_Click"/>
            <Label x:Name="UsernameLabel" Content="Usuário" HorizontalAlignment="Left" Margin="16,121,0,0" VerticalAlignment="Top"/>
            <Label x:Name="PasswordLabel" Content="Senha" HorizontalAlignment="Left" Margin="16,178,0,0" VerticalAlignment="Top"/>
        </Grid>
</UserControl>
